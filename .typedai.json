[
  {
    "baseDir": "./",
    "primary": true,
    "language": "typescript",
    "initialise": "node build.js install",
    "compile": "node build.js build",
    "format": "",
    "staticAnalysis": "node build.js lint",
    "test": "node build.js test",
    "devBranch": "main",
    "summaries": [
      "src/**/*.ts",
      "frontend/src/**/*.ts",
      "bin/**",
      "shared/**"
    ],
    "fileSystemTree": {
      "exclude": [".cache", ".local", ".npm", ".pnpm-store", ".typedai", "frontend/dist"],
      "collapse": ["variables", "test/filesystem", "resources/codeReview"]
    }
  }
]
